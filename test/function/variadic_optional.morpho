// Variadic arguments

fn func(x, y, ...varg, q=-1) {
  print "q=${a}"
  for (a in varg) print a
}

func(1, 2)
// expect: -1

func(1, 2, 3, 4)
// expect: -1
// expect: 3
// expect: 4

func(1, 2, 3, 4, 5, q="Hi")
// expect: Hi
// expect: 3
// expect: 4
// expect: 5

func(1, q=4)
// expect error 'ExpctdArgs'
