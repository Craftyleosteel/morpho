var err = Error("Foo", "A Foo occurred")

try {
  err.throw()
} catch {
  "Foo":
    print "Caught the foo"
    print "(boohoo)"
  "Bar":
    print "Caught the bar"
}

// expect: Caught the foo
// expect: (boohoo)
